generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]  
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["public", "course"]  
}



model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())

  @@schema("public")  
}
